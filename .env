# Production Environment Configuration
ASPNETCORE_ENVIRONMENT=Dev
MYSQL_CONTAINER_NAME=agentic-mysql-prod
APP_CONTAINER_NAME=agentic-backend-prod

# Ports
MYSQL_PORT=3306
APP_HTTP_PORT=5000

# Database
MYSQL_ROOT_PASSWORD=root_password
MYSQL_DATABASE=agentic_db
MYSQL_USER=agentic_user
MYSQL_PASSWORD=agentic_password
MYSQL_BUFFER_POOL_SIZE=512M

# Logging
LOG_LEVEL_DEFAULT=Information
LOG_LEVEL_ASPNETCORE=Warning

# Resources
MEMORY_LIMIT=1G
CPU_LIMIT=1.0
MEMORY_RESERVATION=512M
CPU_RESERVATION=0.5
