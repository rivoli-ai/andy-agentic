# Production Environment Configuration
ASPNETCORE_ENVIRONMENT=Dev
POSTGRES_CONTAINER_NAME=agentic-postgres-prod
APP_CONTAINER_NAME=agentic-backend-prod

# Ports
POSTGRES_PORT=5432
APP_HTTP_PORT=5000

# Database
POSTGRES_DATABASE=agentic_db
POSTGRES_USER=agentic_user
POSTGRES_PASSWORD=agentic_password

# Logging
LOG_LEVEL_DEFAULT=Information
LOG_LEVEL_ASPNETCORE=Warning

# Resources
MEMORY_LIMIT=1G
CPU_LIMIT=1.0
MEMORY_RESERVATION=512M
CPU_RESERVATION=0.5